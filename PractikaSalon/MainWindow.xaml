<Window x:Class="PractikaSalon.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PractikaSalon"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="450" MinWidth="850" MaxWidth= "700" MaxHeight="1200">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60*"/>
            <RowDefinition Height="349*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#FFD642DD">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="10*"/>
                <ColumnDefinition Width="53*"/>
                <ColumnDefinition Width="737*"/>
            </Grid.ColumnDefinitions>

            <Button x:Name="btnBack" HorizontalAlignment="Left" Content="Назад" Click="btnBack_Click" Grid.Column="2" Margin="622,14,0,13"/>
        </Grid>
        <Frame x:Name="FrmMain" Grid.Row="1" NavigationUIVisibility="Hidden" ContentRendered="FrmMain_ContentRendered" />
    </Grid>
</Window>
